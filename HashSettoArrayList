// Remove duplicate elements from the list
package com.mycompany.javap;
import java.util.*;
import java.sql.*;
public class Javap{
  
    public static void main(String args[]) throws SQLException{ 
       HashSet<Integer> list=new HashSet<>(Arrays.asList(3,1,1,6,8,9,22));
        System.out.println("Normal "+list);
        // Hashset to ArrayList
        ArrayList<Integer> farr=new ArrayList<>(list);
        Collections.sort(farr);
         Collections.sort(farr);

        System.out.println("After Sorting"+farr);
     }   
    } 
/* OutPut:-   
    Normal [1, 3, 6, 22, 8, 9]
    After Sorting[1, 3, 6, 8, 9, 22]
*/
